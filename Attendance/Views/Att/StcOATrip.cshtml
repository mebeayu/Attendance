
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link rel="stylesheet" type="text/css" href="~/Scripts/jquery-easyui-1.5.5.6/themes/bootstrap/easyui.css" />
    <link rel="stylesheet" type="text/css" href="~/Scripts/jquery-easyui-1.5.5.6/themes/icon.css">
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/jquery-easyui-1.5.5.6/jquery.easyui.min.js"></script>
    <script src="~/js/oa_trip.js"></script>
    <meta name="viewport" content="width=device-width" />
    <title>StcOATrip</title>
</head>
<body class="easyui-layout" data-options="fit:true">
    <div id="divLayout" class="easyui-layout" style="width:100%;height:100%;">
        <div style="margin:auto;z-index: 9999; position: fixed ! important; left: 5px; top: 4px;width:20%">
            <a id="pro"></a>
        </div>
        <table id="dg" class="easyui-datagrid" style="width:100%;height:95%;text-align:center"
               data-options="singleSelect:true,collapsible:false,toolbar:'#tb1'" rownumbers="false" pagination="false">
            <thead>
                <tr>
                    <th data-options="field:'LASTNAME',resizable:true" width="160px">姓名</th>
                    <th data-options="field:'Days',resizable:true" width="150px">出差天数</th>
                    <th data-options="field:'Title',resizable:true" width="260px">出差事由</th>
                    <th data-options="field:'Path',resizable:true" width="160px">出差区间</th>
                    <th data-options="field:'StartDate',resizable:true" width="200px">开始日期</th>
                    <th data-options="field:'EndDate',resizable:true" width="200px">结束日期</th>
                </tr>
            </thead>
        </table>
    </div>
    <div id="tb1" style="padding:5px;height:auto">
        <input id="LASTNAME" class="easyui-textbox" style="width:200px;height:25px;padding:5px" data-options="
			prompt: '姓名，若为空则为全部'
			">
        <input id="start_time" class="easyui-datebox" data-options="formatter:myformatter,parser:myparser">
        -
        <input id="end_time" class="easyui-datebox" data-options="formatter:myformatter,parser:myparser">
        <a href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-search'" style="width:200px" onclick="Stc_Trip_Days()">统计出差天数</a>
    </div>
</body>
</html>
